#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/upuphone/project_ai/cld-editor/node_modules/.pnpm/antlr4ts-cli@0.5.0-alpha.4/node_modules/antlr4ts-cli/node_modules:/Users/upuphone/project_ai/cld-editor/node_modules/.pnpm/antlr4ts-cli@0.5.0-alpha.4/node_modules:/Users/upuphone/project_ai/cld-editor/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/upuphone/project_ai/cld-editor/node_modules/.pnpm/antlr4ts-cli@0.5.0-alpha.4/node_modules/antlr4ts-cli/node_modules:/Users/upuphone/project_ai/cld-editor/node_modules/.pnpm/antlr4ts-cli@0.5.0-alpha.4/node_modules:/Users/upuphone/project_ai/cld-editor/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../antlr4ts" "$@"
else
  exec node  "$basedir/../../antlr4ts" "$@"
fi
